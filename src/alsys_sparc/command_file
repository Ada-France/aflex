-- # compile shell script for aflex
-- # set the following to match the requirements of your system
-- 
-- setenv COMPILER "ada -e"
-- setenv LINKER "a.ld"
-- 
-- #note: if you have a different suffix for Ada programs the script
-- #change_suffix will do it automatically on a Unix system.
-- 
-- setenv ADA_SUFFIX ".a"
-- 
-- set verbose
-- # compile the vstrings package
COMPILE vstringS.a    
COMPILE vstringB.a

-- # compile instantiations

COMPILE tstrings.a
COMPILE file_strings.a
COMPILE int_io.a    
-- 
COMPILE misc_defsS.a
COMPILE parse_goto.a  
COMPILE nfaS.a  
COMPILE mainS.a  
COMPILE ascan_dfa.a  
COMPILE template_managerS.a  
COMPILE file_managerS.a                    -- *
COMPILE parse_shift_reduce.a  
COMPILE ascan_io.a
COMPILE skeleton_managerS.a
COMPILE command_lineS.a
COMPILE command_lineB.a                    -- *
COMPILE dfaS.a
COMPILE cclS.a
COMPILE parse_tokens.a
COMPILE genS.a
COMPILE miscS.a
COMPILE template_managerB.a
COMPILE file_managerB.a                    -- *
COMPILE miscB.a
COMPILE aflex.a				   -- *
COMPILE cclB.a
COMPILE misc_defsB.a
COMPILE tblcmpS.a
COMPILE skeleton_managerB.a
COMPILE ecsS.a
COMPILE tblcmpB.a
COMPILE dfaB.a
COMPILE nfaB.a
COMPILE ecsB.a
COMPILE symS.a
COMPILE ascan.a
COMPILE aflex_scanner.a
COMPILE parse.a
COMPILE mainB.a
COMPILE genB.a
COMPILE symB.a

BIND aflex

-- unset verbose
